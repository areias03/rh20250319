@article{vermaHumanGutMicrobiota2020,
  title = {Human {{Gut Microbiota}} and {{Mental Health}}: {{Advancements}} and {{Challenges}} in {{Microbe-Based Therapeutic Interventions}}},
  shorttitle = {Human {{Gut Microbiota}} and {{Mental Health}}},
  author = {Verma, Helianthous and Phian, Sonika and Lakra, Priya and Kaur, Jasleen and Subudhi, Sanjukta and Lal, Rup and Rawat, Charu Dogra},
  year = {2020},
