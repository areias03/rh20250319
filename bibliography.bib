  author = {Dmitrijeva, Marija and Tackmann, Janko and Matias Rodrigues, Jo{\~a}o Frederico and {Huerta-Cepas}, Jaime and Coelho, Luis Pedro and {von Mering}, Christian},
  year = {2024},
