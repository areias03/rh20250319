@article{agrenRAVENToolboxIts2013,
  title = {The {{RAVEN Toolbox}} and {{Its Use}} for {{Generating}} a {{Genome-scale Metabolic Model}} for {{Penicillium}} Chrysogenum},
  author = {Agren, Rasmus and Liu, Liming and Shoaie, Saeed and Vongsangnak, Wanwipa and Nookaew, Intawat and Nielsen, Jens},
