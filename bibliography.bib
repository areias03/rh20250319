  file = {/home/alexandre/Zotero/storage/TDVFQ4M3/Santos-Júnior et al. - 2024 - Discovery of antimicrobial peptides in the global microbiome with machine learning.pdf}
}

@article{schmidtSPIRESearchablePlanetaryscale2024,
  title = {{{SPIRE}}: A {{Searchable}},~{{Planetary-scale mIcrobiome REsource}}},
  shorttitle = {{{SPIRE}}},
  author = {Schmidt, Thomas S B and Fullam, Anthony and Ferretti, Pamela and Orakov, Askarbek and Maistrenko, Oleksandr M and Ruscheweyh, Hans-Joachim and Letunic, Ivica and Duan, Yiqian and Van~Rossum, Thea and Sunagawa, Shinichi and Mende, Daniel R and Finn, Robert D and Kuhn, Michael and Pedro~Coelho, Luis and Bork, Peer},
