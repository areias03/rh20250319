@article{santos-juniorDiscoveryAntimicrobialPeptides2024,
  title = {Discovery of Antimicrobial Peptides in the Global Microbiome with Machine Learning},
  author = {{Santos-J{\'u}nior}, C{\'e}lio Dias and Torres, Marcelo D.T. and Duan, Yiqian and Rodr{\'i}guez Del R{\'i}o, {\'A}lvaro and Schmidt, Thomas S.B. and Chong, Hui and Fullam, Anthony and Kuhn, Michael and Zhu, Chengkai and Houseman, Amy and Somborski, Jelena and Vines, Anna and Zhao, Xing-Ming and Bork, Peer and {Huerta-Cepas}, Jaime and {De La Fuente-Nunez}, Cesar and Coelho, Luis Pedro},
  year = {2024},
