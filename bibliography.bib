Read\_Status\_Date: 2025-01-15T00:06:47.824Z},
  file = {/home/alexandre/Zotero/storage/GEUX6YTC/Cheng et al. - 2023 - Machine learning for metabolic pathway optimization A review.pdf}
}

@article{chenReconstructionSimulationAnalysis2024,
  title = {Reconstruction, Simulation and Analysis of Enzyme-Constrained Metabolic Models Using {{GECKO Toolbox}} 3.0},
  author = {Chen, Yu and Gustafsson, Johan and Tafur Rangel, Albert and Anton, Mihail and Domenzain, Iv{\'a}n and Kittikunapong, Cheewin and Li, Feiran and Yuan, Le and Nielsen, Jens and Kerkhoven, Eduard J.},
  year = {2024},
  month = mar,
  journal = {Nature Protocols},
  volume = {19},
  number = {3},
  pages = {629--667},
  publisher = {Nature Publishing Group},
  issn = {1750-2799},
  doi = {10.1038/s41596-023-00931-7},
  urldate = {2025-04-02},
  abstract = {Genome-scale metabolic models (GEMs) are computational representations that enable mathematical exploration of metabolic behaviors within cellular and environmental constraints. Despite their wide usage in biotechnology, biomedicine and fundamental studies, there are many phenotypes that GEMs are unable to correctly predict. GECKO is a method to improve the predictive power of a GEM by incorporating enzymatic constraints using kinetic and omics data. GECKO has enabled reconstruction of enzyme-constrained metabolic models (ecModels) for diverse organisms, which show better predictive performance than conventional GEMs. In this protocol, we describe how to use the latest version GECKO 3.0; the procedure has five stages: (1) expansion from a starting metabolic model to an ecModel structure, (2) integration of enzyme turnover numbers into the ecModel structure, (3) model tuning, (4) integration of proteomics data into the ecModel and (5) simulation and analysis of ecModels. GECKO 3.0 incorporates deep learning-predicted enzyme kinetics, paving the way for improved metabolic models for virtually any organism and cell line in the absence of experimental data. The time of running the whole protocol is organism dependent, e.g., {\textasciitilde}5 h for yeast.},
  copyright = {2024 Springer Nature Limited},
  langid = {english},
  keywords = {Biochemical networks,Computational models,Machine learning,Metabolic engineering},
  file = {/home/alexandre/Zotero/storage/KKKYY75Z/Chen et al. - 2024 - Reconstruction, simulation and analysis of enzyme-constrained metabolic models using GECKO Toolbox 3.pdf}
}

