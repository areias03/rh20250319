Read\_Status\_Date: 2025-01-10T04:18:01.321Z},
  file = {/home/alexandre/Zotero/storage/EWAPSN6X/Li et al. - 2022 - Deep learning-based kcat prediction enables improved enzyme-constrained model reconstruction.pdf}
}

