  keywords = {Bacteria,Bordetella pertussis,Genes Bacterial,Genome Bacterial,Genome-scale metabolic models,Genome-scale metabolic reconstruction,Lactobacillus plantarum,Metabolic Networks and Pathways,Pseudomonas putida,Software,Systematic evaluation},
