@article{mendozaSystematicAssessmentCurrent2019,
  title = {A Systematic Assessment of Current Genome-Scale Metabolic Reconstruction Tools},
  author = {Mendoza, Sebasti{\'a}n N. and Olivier, Brett G. and Molenaar, Douwe and Teusink, Bas},
  year = {2019},
