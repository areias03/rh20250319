@article{caivanoEnzymeconstrainedMetabolicModel2023a,
  title = {Enzyme-Constrained Metabolic Model and {\emph{in Silico}} Metabolic Engineering of {{{\emph{Clostridium}}}}{\emph{ Ljungdahlii}} for the Development of Sustainable Production Processes},
  author = {Caivano, Antonio and {van Winden}, Wouter and Dragone, Giuliano and Mussatto, Solange I.},
  year = {2023},
  month = jan,
  journal = {Computational and Structural Biotechnology Journal},
  volume = {21},
  pages = {4634--4646},
  issn = {2001-0370},
  doi = {10.1016/j.csbj.2023.09.015},
  urldate = {2025-04-02},
  abstract = {Constraint-based genome-scale models (GEMs) of microorganisms provide a powerful tool for predicting and analyzing microbial phenotypes as well as for understanding how these are affected by genetic and environmental perturbations. Recently, MATLAB and Python-based tools have been developed to incorporate enzymatic constraints into GEMs. These constraints enhance phenotype predictions by accounting for the enzyme cost of catalyzed model{\textasciiacute}s reactions, thereby reducing the space of possible metabolic flux distributions. In this study, enzymatic constraints were added to an existing GEM of Clostridium ljungdahlii, a model acetogenic bacterium, by including its enzyme turnover numbers (kcats) and molecular masses, using the Python-based AutoPACMEN approach. When compared to the metabolic model iHN637, the enzyme cost-constrained model (ec\_iHN637) obtained in our study showed an improved predictive ability of growth rate and product profile. The model ec\_iHN637 was then employed to perform in silico metabolic engineering of C. ljungdahlii, by using the OptKnock computational framework to identify knockouts to enhance the production of desired fermentation products. The in silico metabolic engineering was geared towards increasing the production of fermentation products by C. ljungdahlii, with a focus on the utilization of synthesis gas and CO2. This resulted in different engineering strategies for overproduction of valuable metabolites under different feeding conditions, without redundant knockouts for different products. Importantly, the results of the in silico engineering results indicated that the mixotrophic growth of C. ljungdahlii is a promising approach to coupling improved cell growth and acetate and ethanol productivity with net CO2 fixation.},
  keywords = {Acetogenic bacteria,CO2 fixation,Flux balance analysis,Genome-scale model,Metabolic engineering,OptKnock},
  file = {/home/alexandre/Zotero/storage/FZSJMV73/Caivano et al. - 2023 - Enzyme-constrained metabolic model and in silico metabolic engineering of Clostridium ljun.pdf;/home/alexandre/Zotero/storage/ZYRKN4YD/S2001037023003276.html}
}

@article{cammarotaGutMicrobiomeBig2020,
  title = {Gut Microbiome, Big Data and Machine Learning to Promote Precision Medicine for Cancer},
  author = {Cammarota, Giovanni and Ianiro, Gianluca and Ahern, Anna and Carbone, Carmine and Temko, Andriy and Claesson, Marcus J. and Gasbarrini, Antonio and Tortora, Giampaolo},
